function getCardSprite(c){
	switch(c){
		case card.baseBullet: return spr_card_baseBullet;
		case card.splittingBullet: return spr_card_splittingBullet;
		case card.baseBolt: return spr_card_baseBolt;
		case card.boulder: return spr_card_boulder;
		case card.axe: return spr_card_axe;
		case card.zapper: return spr_card_zapper;
		case card.costDown: return spr_card_costDown;
		case card.weakeningRay: return spr_card_weakeningRay;
		case card.radarBomb: return spr_card_radarBomb;
		case card.portalDrill: return spr_card_portalDrill;
		case card.vampiricThirst: return spr_card_vampiricThirst;
		case card.openingShot: return spr_card_openingShot;
		case card.shieldingWallDestroy: return spr_card_shieldingWallDestroy;
		case card.shieldingWallBounce: return spr_card_shieldingWallBounce;
		case card.barrier: return spr_card_barrier;
	}
}